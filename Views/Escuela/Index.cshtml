@model Escuela

@{
    ViewData["Title"] = "Información Escuela";
}

<div class="text-center">
    <h1 class="display-4">Escuela: @ViewData["Title"]</h1>
    <h2>Nombre: @Model.Nombre</h2>
    <h3>Tipo de Escuela: @Model</h3>
    <address>
        <p>País: @Model.Pais</p>
        <p>Ciudad: @Model.Ciudad</p>
    </address>

    <p><strong>Año fundación: @Model.AnioDeCreacion</strong></p>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
